<ContentDialog
    x:Class="MyRDP.Views.EditConnectionGroupDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Edit Group"
    PrimaryButtonText="Save"
    CloseButtonText="Cancel">

    <StackPanel>
        <TextBox Header="Group Name" Text="{x:Bind Group.Name, Mode=TwoWay}"/>
    </StackPanel>
</ContentDialog>